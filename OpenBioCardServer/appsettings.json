{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "Type": "PgSQL",
    "ConnectionString": "Host=localhost;Database=openbiocard;Username=postgres;Password=postgres"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "https://example.com/"
    ]
  },
  "CacheSettings": {
    "Enabled": true,
    "UseRedis": false,
    "RedisConnectionString": "localhost:6379",
    "InstanceName": "OpenBioCard:",
    "CacheSizeLimit": 300,
    "ExpirationMinutes": 30,
    "SlidingExpirationMinutes": 5
  },
  "CompressionSettings": {
    "Enabled": true,
    "EnableForHttps": true,
    "Level": "Fastest"
  },
  "AuthSettings": {
    "RootUsername": "root",
    "RootPassword": "change_me_in_production_env_vars"
  },
  "AssetSettings": {
    "MaxFileSizeMB": 5,
    "AllowedImageTypes": [
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/webp"
    ]
  }
}
