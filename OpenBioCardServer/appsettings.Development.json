{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseSettings": {
    "Type": "SQLite",
    "ConnectionString": "Data Source=development.db"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:8787"
    ]
  },
  "CacheSettings": {
    "Enabled": true,
    "UseRedis": false,
    "RedisConnectionString": "localhost:6379",
    "InstanceName": "OpenBioCard:",
    "CacheSizeLimit": 200,
    "ExpirationMinutes": 5,
    "SlidingExpirationMinutes": 2,
    "FactorySoftTimeoutMilliseconds": 500,
    "EnableFailSafe": true,
    "FailSafeMaxDurationMinutes": 120,
    "FailSafeThrottleDurationSeconds": 30,
    "DistributedCacheCircuitBreakerDurationSeconds": 2
  },
  "CompressionSettings": {
    "Enabled": true,
    "EnableForHttps": true,
    "Level": "Fastest"
  },
  "AuthSettings": {
    "RootUsername": "root",
    "RootPassword": "password"
  },
  "AssetSettings": {
    "MaxFileSizeMB": 5,
    "AllowedImageTypes": [
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/webp"
    ]
  },
  "RateLimitSettings": {
    "Policies": [
      {
        "PolicyName": "login",
        "Type": "FixedWindow",
        "PermitLimit": 1000,
        "WindowSeconds": 60,
        "QueueLimit": 10
      },
      {
        "PolicyName": "general",
        "Type": "FixedWindow",
        "PermitLimit": 10000,
        "WindowSeconds": 60,
        "QueueLimit": 100
      }
    ]
  }
}
